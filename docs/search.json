---
---
[
  {% for post in site.posts %}
    {

      "title"    : "{{ post.title | strip_html | escape }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "category" : "{{post.categories | join: ', '}}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "date"     : "{{ post.date }}",
      "description" : "{{post.description | strip_html | strip_newlines | escape | replace "\", "\\\\" }}",
      "content" : "{{post.content | strip_html | strip_newlines | escape | replace "\\", "\\" }}",
      "content2" : "{{post.content | strip_html | strip_newlines | escape | replace "\\", "\\\" }}",
        "content3" : "{{post.content | strip_html | strip_newlines | escape | replace "\\", "\\\\" }}",
  
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
